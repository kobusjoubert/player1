= form_for @album_artist do |f|
  - if @album_artist.errors.any?
    #error_explanation
      %h2= "#{pluralize(@album_artist.errors.count, "error")} prohibited this album_artist from being saved:"
      %ul
        - @album_artist.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :album
    = f.text_field :album
  .field
    = f.label :artist
    = f.text_field :artist
  .actions
    = f.submit 'Save'
